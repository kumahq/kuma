{
  "cniVersion": "0.3.0",
  "name": "k8s-pod-network",
  "plugins": [
    {
      "etcd_endpoints": "http://10.0.0.1:2379",
      "ipam": {
        "type": "calico-ipam"
      },
      "kubernetes": {
        "kubeconfig": "/etc/cni/net.d/calico-kubeconfig"
      },
      "log_level": "info",
      "mtu": 1500,
      "policy": {
        "type": "k8s"
      },
      "type": "calico"
    },
    {
      "capabilities": {
        "portMappings": true
      },
      "snat": true,
      "type": "portmap"
    },
    {
      "kubernetes": {
        "cni_bin_dir": "/opt/cni/bin",
        "exclude_namespaces": [
          "kuma-system"
        ],
        "kubeconfig": "/foo/bar/ZZZ-kuma-cni-kubeconfig"
      },
      "log_level": "info",
      "type": "kuma-cni"
    }
  ]
}