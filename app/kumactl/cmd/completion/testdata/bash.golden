    flags+=("--config-file=")
    two_word_flags+=("--config-file")
    flags+=("--log-level=")
    two_word_flags+=("--log-level")
    flags+=("--no-config")

    must_have_one_flag=()
    must_have_one_flag+=("--file=")
    must_have_one_flag+=("-f")
