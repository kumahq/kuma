{
  "items": [
    {
      "mesh": "default",
      "name": "web1-to-backend1",
      "rules": [
        {
          "sources": [
            {
              "match": {
                "service": "web1",
                "version": "1.0"
              }
            }
          ],
          "destinations": [
            {
              "match": {
                "service": "backend1",
                "env": "dev"
              }
            }
          ],
          "conf": {
            "backend": "file"
          }
        }
      ],
      "type": "TrafficLogging"
    },
    {
      "mesh": "default",
      "name": "web2-to-backend2",
      "rules": [
        {
          "sources": [
            {
              "match": {
                "service": "web2",
                "version": "1.0"
              }
            }
          ],
          "destinations": [
            {
              "match": {
                "service": "backend2",
                "env": "dev"
              }
            }
          ],
          "conf": {
            "backend": "logstash"
          }
        }
      ],
      "type": "TrafficLogging"
    }
  ]
}
