{
  "type": "MeshMultiZoneService",
  "name": "http.bin",
  "mesh": "default",
  "spec": {
    "selector": {
      "meshService": {
        "matchLabels": {
          "kuma.io/display-name": "httpbin"
        }
      }
    },
    "ports": [
      {
        "port": 8080,
        "appProtocol": "http"
      }
    ]
  }
}
