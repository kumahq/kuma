{
 "type": "HealthCheck",
 "mesh": "default",
 "name": "web-to-backend",
 "creationTime": "0001-01-01T00:00:00Z",
 "modificationTime": "0001-01-01T00:00:00Z",
 "labels": {
  "kuma.io/origin": "zone"
 },
 "sources": [
  {
   "match": {
    "kuma.io/service": "web"
   }
  }
 ],
 "destinations": [
  {
   "match": {
    "kuma.io/service": "backend"
   }
  }
 ],
 "conf": {
  "interval": "10s",
  "timeout": "2s",
  "unhealthyThreshold": 3,
  "healthyThreshold": 1
 }
}
