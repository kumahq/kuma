{
 "policies": [
  {
   "kind": "MeshAccessLog",
   "origins": [
    {
     "kri": "kri_mal_default___multiple-backends_"
    }
   ],
   "rules": [
    {
     "conf": {
      "backends": [
       {
        "type": "File",
        "file": {
         "format": {
          "type": "Plain",
          "plain": "[%START_TIME%]",
          "omitEmptyValues": false
         },
         "path": "/dev/stdout"
        }
       }
      ]
     },
     "kri": "kri_mal_default___multiple-backends_"
    }
   ]
  }
 ]
}
