{
 "policies": [
  {
   "kind": "MeshAccessLog",
   "origins": [
    {
     "kri": "kri_mal_default___multiple-backends_"
    }
   ],
   "rules": [
    {
     "conf": {
      "backends": [
       {
        "type": "File",
        "file": {
         "format": {
          "type": "Plain",
          "plain": "[%START_TIME%]",
          "omitEmptyValues": false
         },
         "path": "/dev/stdout"
        }
       }
      ]
     },
     "kri": "kri_mal_default___multiple-backends_"
    }
   ]
  },
  {
   "kind": "MeshRateLimit",
   "origins": [
    {
     "kri": "kri_mrl_default___backend-rate-limit_"
    }
   ],
   "rules": [
    {
     "conf": {
      "local": {
       "http": {
        "requestRate": {
         "num": 5,
         "interval": "10s"
        },
        "onRateLimit": {
         "status": 423,
         "headers": {
          "set": [
           {
            "name": "x-kuma-rate-limited",
            "value": "true"
           }
          ]
         }
        }
       }
      }
     },
     "kri": "kri_mrl_default___backend-rate-limit_"
    }
   ]
  }
 ]
}
